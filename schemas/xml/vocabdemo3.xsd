           attributeFormDefault="unqualified" version="draft10">

   <xs:element name="Applicability" type="rsm:Applicability">
      <xs:annotation>
         <xs:documentation>
           a root element for a document describing a Resource,
           an identified, described, and discoverable component of the 
           distributed data environment. 
         </xs:documentation>
      </xs:annotation>
   </xs:element>
   
   <xs:complexType name="Applicability">
     <xs:annotation>
       <xs:documentation>
         metadata describing the resource's applicability to a particular
         domain.
       </xs:documentation>
     </xs:annotation>

     <xs:sequence>
       
     </xs:sequence>
   </xs:complexType>

   <xs:complexType name="MaterialsScience">
     <xs:annotation>
       <xs:documentation>
         Information describing a resource's applicability to Materials Science
       </xs:documentation>
     </xs:annotation>

     <xs:complexContent>
       <xs:extension base="rsm:Applicability">
         <xs:sequence>
           <xs:element name="materialType" type="mse:Material_type" 
                       minOccurs="0" maxOccurs="unbounded">
             <xs:annotation>
               <xs:documentation>
                 the category of material studied in the data contained in 
                 this resource.
               </xs:documentation>
             </xs:annotation>
           </xs:element>

           <xs:element name="structuralFeature" 
                       type="mse:Structural_Feature" 
                       minOccurs="0" maxOccurs="unbounded">
             <xs:annotation>
               <xs:documentation>
                 the primary or prevalent characteristic of the structure of 
                 the material of interest to this resource
               </xs:documentation>
             </xs:annotation>
           </xs:element>

           <xs:element name="propertyAddressed" type="mse:Properties_addressed"
                       minOccurs="0" maxOccurs="unbounded">
             <xs:annotation>
               <xs:documentation>
                 a category of property that is sampled by the contained data
               </xs:documentation>
             </xs:annotation>
           </xs:element>

           <xs:element name="experimentalMethod" type="mse:Experimental_Methods"
                       minOccurs="0" maxOccurs="unbounded">
             <xs:annotation>
               <xs:documentation>
                  the experimental technique used to acquire the data
                  within this resource 
               </xs:documentation>
             </xs:annotation>
           </xs:element>

           <xs:element name="computationalMethod" type="mse:Computational_method"
                       minOccurs="0" maxOccurs="unbounded">
             <xs:annotation>
               <xs:documentation>
                  the computational technique used to acquire the data
                  within this resource 
               </xs:documentation>
             </xs:annotation>
           </xs:element>

           <xs:element name="synthesisProcessing"
                       type="mse:Synthesis_and_Processing"
                       minOccurs="0" maxOccurs="unbounded">
             <xs:annotation>
               <xs:documentation>
                 the physical processing or preparation technique applied 
                 to the material being studied.
               </xs:documentation>
             </xs:annotation>
           </xs:element>

         </xs:sequence>
       </xs:extension>
     </xs:complexContent>
   </xs:complexType>

